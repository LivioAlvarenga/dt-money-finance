{
  "name": "template-nextapp-typescript-tailwind",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:server": "node src/lib/json-server.js",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "studio": "npx prisma studio -b chrome -p 5173",
    "generate": "npx prisma generate",
    "migrate": "npx prisma migrate dev",
    "seed": "npx prisma db seed",
    "start-docker": "docker-compose up -d",
    "stop-docker": "docker-compose stop",
    "test": "vitest run --dir src/use-cases",
    "test:watch": "vitest --dir src/use-cases",
    "test:ui": "vitest --ui --no-threads",
    "test:coverage": "vitest run --coverage --no-threads",
    "test:e2e": "vitest run --dir src/http/controllers --no-threads",
    "test:safety": "vitest run --dir src/http/middlewares"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
